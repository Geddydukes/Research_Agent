# Google Gemini API Configuration
GOOGLE_API_KEY=your_google_api_key_here
SEMANTIC_SCHOLAR_API_KEY=your_semantic_scholar_api_key_here
# GEMINI_EMBEDDING_MODEL=gemini-embedding-001

# Supabase Database Configuration
SUPABASE_URL=your_supabase_url_here
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key_here
# Required for sign-in: use the publishable / anon public key (Dashboard → API → anon public). Do not use the service_role secret here.
SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Encryption for BYOK (required in production)
ENCRYPTION_KEY=change_me_in_production

# Model Selection (optional - defaults are set in src/agents/config.ts)
# Uncomment and set if you want to override the defaults
# INGESTION_MODEL=gemini-2.5-flash
# ENTITY_EXTRACTION_MODEL=gemini-2.5-flash
# RELATIONSHIP_EXTRACTION_MODEL=gemini-2.5-flash
# VALIDATION_MODEL=gemini-2.5-pro
# REASONING_MODEL=gemini-2.5-pro

PDF_PARSE_CONFIDENCE_THRESHOLD=0.7

# API Server Configuration
API_PORT=3000
API_HOST=0.0.0.0
# API_KEY is deprecated for Supabase Auth-only mode
# API_KEY=your_api_key_here
CORS_ORIGIN=http://localhost:3001
LOG_LEVEL=info
NODE_ENV=development
PIPELINE_RATE_LIMIT_PER_MINUTE=10
